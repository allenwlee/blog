<div class="container">
  <div class="row_fluid">
    <div class="span9">
      <h1>Edit Post</h1>
      <form method="post" action="/post/<%=@post.id%>/edit">
        <input type="text" name="title" placeholder="title">
        <input type="textarea" name="body" placeholder="body"></textarea>
        <input type="text" name="category" placeholder="tag">
      </form>
    </div>
  </div>
  <div class="span9">
    <%=erb :_tags, :layout => false %>
  </div>
</div>
