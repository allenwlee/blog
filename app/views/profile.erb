<div class="container">
  <div class="row-fluid">
    <div class="span9">
      <h1>Yer Profile</h1>
      <h3><%= current_user.email %></h3>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span3">
      <% @posts.each do |p| %>
        <%= erb :_post, :locals => { :title => p.title, :body => p.body, :email => User.find(p.user_id).email } %>  
      <% end %>
    </div>
  </div>
</div>
